    "clean": "rimraf dist",
    "build:ssr": "npm run clean && npm run build:ssr:browser && npm run build:ssr:server && npm run webpack:server",
    "build:ssr:browser": "ng build --prod --aot --app browser",
    "build:ssr:server": "ng build --prod --aot --app server --output-hashing none",
    "serve:ssr": "node -r dotenv/config dist/server.js dotenv_config_path=ssr/.env",
    "webpack:server": "webpack --config ssr/webpack.server.config.js --progress --colors"
